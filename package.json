    "compare-semver": "1.1.0",
    "clipboardy": "2.3.0",
    "semver": "7.3.4",
    "table": "6.0.7",
    "yargs": "16.2.0",
    "ts-node": "9.1.1"
  },
  "devDependencies": {
    "@types/node": "14.14.33",
    "@types/table": "6.0.0",
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "eslint": "7.21.0",
    "npm-check-updates": "11.2.2",
    "prettier": "2.2.1",
    "cross-env-file": "1.0.0",
    "env-cmd": "10.1.0",
    "tslib": "2.1.0",
    "jest": "26.6.3",
    "ts-jest": "26.5.4",
    "typescript": "4.2.3",
    "npm-run-all": "4.1.5"
  }
