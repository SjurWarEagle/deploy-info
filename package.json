{
  "name": "version-info",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start txt": "ts-node src/starter.ts --format='txt'",
    "start markdown": "ts-node src/starter.ts --format='md'",
    "lint": "eslint src/**/* --format visualstudio",
    "lint fix": "eslint src/**/* --format visualstudio --fix",
    "prettier": "prettier --write src",
    "test": "jest",
    "concourse-deploy": "env-cmd -f .env --use-shell c:/Dropbox/PortableApps/fly.exe -t tutorial set-pipeline -p version-info --non-interactive -c concourse/pipeline.yml --var \"GITHUB_USER=%GITHUB_USER%\" --var \"GITHUB_TOKEN=%GITHUB_TOKEN%\" --var \"GIST_URL=%GIST_URL%\"",
    "concourse-destroy": "c:/Dropbox/PortableApps/fly -t tutorial destroy-pipeline -p version-info",
    "concourse-unpause-pipeline": "c:/Dropbox/PortableApps/fly.exe -t tutorial unpause-pipeline -p version-info",
    "dependencies:update": "ncu -u -x typescript,ngx-mqtt",
    "dependencies:install": "npm install",
    "update:dependencies and build": "run-s dependencies:update dependencies:install"
  },
  "keywords": [],
  "author": "",
